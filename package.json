{
  "name": "agribridge",
  "version": "1.0.0",
  "description": "A platform connecting natural farmers with conscious consumers",
  "main": "Backend-AgriBridge/dist/server.js",
  "scripts": {
    "preinstall": "cd Backend-AgriBridge && npm install",
    "build": "cd Backend-AgriBridge && tsc",
    "start": "node Backend-AgriBridge/dist/server.js",
    "dev": "cd Backend-AgriBridge && npm run dev",
    "migration:generate": "cd Backend-AgriBridge && npm run migration:generate",
    "migration:run": "cd Backend-AgriBridge && npm run migration:run",
    "migration:revert": "cd Backend-AgriBridge && npm run migration:revert",
    "seed": "cd Backend-AgriBridge && npm run seed",
    "create-admin": "cd Backend-AgriBridge && npm run create-admin"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "private": true,
  "workspaces": [
    "Backend-AgriBridge"
  ]
} 